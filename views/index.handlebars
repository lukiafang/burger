<h1>Burgers!</h1>

<h2>World Class of Burgers</h2>



<ul>
  {{#each burgers}}<li>
	{{burger_name}}
	<button class="delete-burger" data-id="{{this.id}}">Devoure!</button>
  </li>{{/each}}

</ul>




<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>



  <button type="submit">Add Burger</button>
</form>

